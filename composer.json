{
  "name": "whyl/laravel-api-scaffolder",
  "description": "Artisan commands to scaffold Services and Repositories for a standard API structure.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Whylgher Lima",
      "email": "whylgher@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1|^8.2",
    "illuminate/support": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Whyl\\ApiScaffolder\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Whyl\\ApiScaffolder\\ApiScaffolderServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
